<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.degreetracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Asset Manager"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <receiver
            android:name="com.example.AssetManagementSystem.Receivers.ExtendedWarrantyExpirationReceiver"
            android:enabled="true"
            android:exported="true"/>
        <receiver
            android:name="com.example.AssetManagementSystem.Receivers.ProductMFGWarrantyEndReceiver"
            android:enabled="true"
            android:exported="true" />
        <receiver
            android:name="com.example.AssetManagementSystem.Receivers.CourseStartReceiver"
            android:enabled="true"
            android:exported="true" />

        <activity android:name="com.example.AssetManagementSystem.Activites.ModifyViews.ModifyNoteActivity" />
        <activity android:name="com.example.AssetManagementSystem.Activites.DetailViews.DetailNoteActivity" />
        <activity android:name="com.example.AssetManagementSystem.Activites.DetailViews.DetailCampusType" />
        <activity android:name="com.example.AssetManagementSystem.Activites.DetailViews.DetailVendorActivity" />
        <activity android:name="com.example.AssetManagementSystem.Activites.DetailViews.DetailProductActivity" />
        <activity android:name="com.example.AssetManagementSystem.Activites.DetailViews.DetailWarrantyActivity" />
        <activity android:name="com.example.AssetManagementSystem.Activites.ModifyViews.ModifyVendorActivity" />
        <activity android:name="com.example.AssetManagementSystem.Activites.ModifyViews.ModifyWarrantyActivity" />
        <activity android:name="com.example.AssetManagementSystem.Activites.ModifyViews.ModifyProductActivity" />
        <activity android:name="com.example.AssetManagementSystem.Activites.ModifyViews.ModifyCampusActivity" />
        <activity
            android:name="com.example.AssetManagementSystem.Activites.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>